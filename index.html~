      setInterval(change, 15000);
      function change() {
	  counter1 = between(0, text.length - 1);	  
	  elem.innerHTML = text[counter1];
          //counter1++;
          //if(counter1 >= text.length) { counter1 = 0; }

	  function between(min, max) {  
	      return Math.floor(
		  Math.random() * (max - min + 1) + min
	      )
	  }
	  // Imprime en consola
	  console.log(counter1)
      }

    </script>
    
    
